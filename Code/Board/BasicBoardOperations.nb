   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"curPoint", "=", 
        RowBox[{"{", 
         RowBox[{"x", ",", "y"}], "}"}]}], ",", 
       RowBox[{"\[Theta]", "=", 
        FractionBox["\[Pi]", "4"]}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"-", 
             RowBox[{"Cot", "[", 
              FractionBox[
               RowBox[{"3", "\[Pi]"}], "8"], "]"}]}], 
            RowBox[{
            "curPoint", "\[LeftDoubleBracket]", "2", 
             "\[RightDoubleBracket]"}]}], "<", 
           RowBox[{
           "curPoint", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}],
            "<", 
           RowBox[{
            RowBox[{"Cot", "[", 
             FractionBox[
              RowBox[{"3", "\[Pi]"}], "8"], "]"}], 
            RowBox[{
            "curPoint", "\[LeftDoubleBracket]", "2", 
