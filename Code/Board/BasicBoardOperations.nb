Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Helper", " ", "function", " ", "to", " ", "figure", " ", "out", " ", 
    "which", " ", "tile", " ", "the", " ", "point", " ", "is", " ", "in", " ",
     "given", " ", "the", " ", "row"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"findTile", "[", 
    RowBox[{"row_", ",", "point_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"tempPoint", "=", 
        RowBox[{"point", "+", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"row", "-", "1"}], ")"}], "*", 
            RowBox[{"Cos", "[", 
             RowBox[{"3", 
              RowBox[{"\[Pi]", "/", "8"}]}], "]"}]}], ",", 
           RowBox[{
            RowBox[{"-", 
             RowBox[{"(", 
              RowBox[{"row", "-", "1"}], ")"}]}], 
            RowBox[{"Sin", "[", 
             FractionBox[
              RowBox[{"3", "\[Pi]"}], "8"], "]"}]}]}], "}"}]}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"checkerEq", "=", 
        RowBox[{
         RowBox[{"ArcCos", "[", 
          RowBox[{"Cos", "[", 
           RowBox[{
            RowBox[{"(", "x", ")"}], 
            RowBox[{"(", 
             RowBox[{"\[Pi]", "/", 
              RowBox[{"Cos", "[", 
               RowBox[{"3", 
                RowBox[{"\[Pi]", "/", "8"}]}], "]"}]}], ")"}]}], "]"}], "]"}],
          "*", 
         RowBox[{
          RowBox[{"Sin", "[", 
           RowBox[{"3", 
            RowBox[{"\[Pi]", "/", "8"}]}], "]"}], "/", "\[Pi]"}]}]}]}], "}"}],
      ",", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
         "tempPoint", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], 
         "<", "checkerEq"}], "/.", 
        RowBox[{"{", 
         RowBox[{"x", "\[Rule]", 
          RowBox[{
          "tempPoint", "\[LeftDoubleBracket]", "1", 
           "\[RightDoubleBracket]"}]}], "}"}]}], ",", 
       RowBox[{"(*", 
        RowBox[{"Pointing", " ", "up", " ", "triangle"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Return", "[", 
         RowBox[{"2", "*", 
          RowBox[{"Ceiling", "[", 
           RowBox[{
            RowBox[{
            "tempPoint", "\[LeftDoubleBracket]", "1", 
             "\[RightDoubleBracket]"}], "/", 
            RowBox[{"(", 
             RowBox[{"2", 
              RowBox[{"Cos", "[", 
               RowBox[{"3", 
                RowBox[{"\[Pi]", "/", "8"}]}], "]"}]}], ")"}]}], "]"}]}], 
         "]"}], ";"}], "\[IndentingNewLine]", ",", 
       RowBox[{"(*", 
        RowBox[{"Pointing", " ", "down", " ", "triangle"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Return", "[", 
         RowBox[{
          RowBox[{"2", "*", 
           RowBox[{"Round", "[", 
            RowBox[{
             RowBox[{
             "tempPoint", "\[LeftDoubleBracket]", "1", 
