             RowBox[{"(", 
              RowBox[{"2", 
               RowBox[{"Cos", "[", 
                RowBox[{"3", 
                 RowBox[{"\[Pi]", "/", "8"}]}], "]"}]}], ")"}]}], "]"}]}], 
          "+", "1"}], "]"}], ";"}]}], "\[IndentingNewLine]", "]"}]}], 
