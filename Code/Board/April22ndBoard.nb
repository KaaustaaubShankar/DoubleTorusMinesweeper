Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Error", " ", "checking", " ", "to", " ", "ensure", " ", "we", " ", "do", 
    " ", "not", " ", "add", " ", "any", " ", "invalid", " ", 
    RowBox[{"neighbors", ".", " ", "Less"}], " ", "cases", " ", "to", " ", 
    "handle", " ", "manually"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"SetAttributes", "[", 
    RowBox[{"addNeighbor", ",", "HoldFirst"}], "]"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"addNeighbor", "[", 
     RowBox[{
     "neighborLst_", ",", "numRows_", ",", "sector_", ",", "row_", ",", 
      "tile_"}], "]"}], ":=", "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"1", "\[LessEqual]", "sector", "\[LessEqual]", "8"}], "&&", 
       RowBox[{"1", "\[LessEqual]", "row", "\[LessEqual]", "numRows"}], "&&", 
       RowBox[{"1", "\[LessEqual]", "tile", "\[LessEqual]", 
        RowBox[{
         RowBox[{"2", "*", "row"}], "-", "1"}]}], "&&", 
       RowBox[{"!", 
        RowBox[{"MemberQ", "[", 
         RowBox[{"neighborLst", ",", 
          RowBox[{"{", 
           RowBox[{"sector", ",", "row", ",", "tile"}], "}"}]}], "]"}]}]}], 
      ",", 
      RowBox[{"AppendTo", "[", 
       RowBox[{"neighborLst", ",", 
        RowBox[{"{", 
         RowBox[{"sector", ",", "row", ",", "tile"}], "}"}]}], "]"}]}], 
     "\[IndentingNewLine]", "]"}]}]}]}]], "Input",
